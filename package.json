{
  "babel": {
    "env": {
      "es": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      },
      "esnext": {
        "presets": [
          "es2016",
          "es2017",
          "flow",
          "react",
          "stage-0"
        ]
      },
      "node": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "6.0.0"
              }
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      },
      "test": {
        "presets": [
          "env",
          "flow",
          "react",
          "stage-0"
        ]
      },
      "rollup": {
        "presets": [
          [
            "env",
            {
              "modules": false
            }
          ],
          "flow",
          "react",
          "stage-0"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^20.0.4",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-uglify": "^2.0.1",
    "@skatejs/bore": "^4.0.0",
    "@skatejs/renderer-lit-html": "^0.0.0",
    "@skatejs/renderer-preact": "^0.1.0",
    "@skatejs/renderer-react": "^0.0.0",
    "@skatejs/sk-router": "^0.1.0",
    "@skatejs/ssr": "^0.15.4",
    "@skatejs/val": "^0.3.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-modules-map": "1.0.0",
    "babel-plugin-modules-web-compat": "^1.1.1",
    "babel-plugin-transform-skate-flow-props": "^0.1.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "6.24.1",
    "conartist": "^0.12.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "file-loader": "^1.1.5",
    "highlight.js": "^9.12.0",
    "husky": "^0.13.3",
    "img-loader": "^2.0.0",
    "jest-cli": "^21.0.2",
    "lint-staged": "^4.0.2",
    "lit-html": "^0.7.1",
    "preact": "^8.2.5",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.1",
    "prismjs": "^1.8.4",
    "raw-loader": "^0.5.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "typescript": "^2.3.3",
    "typescript-formatter": "^5.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "worker-loader": "^1.1.0"
  },
  "files": [
    "es/",
    "esnext/",
    "node/",
    "umd/"
  ],
  "main": "node/index.js",
  "module": "es/index.js",
  "esnext": "esnext/index.js",
  "scripts": {
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:esnext": "BABEL_ENV=esnext babel src --out-dir esnext",
    "build:node": "BABEL_ENV=node babel src --out-dir node",
    "test": "jest",
    "build:umd": "BABEL_ENV=rollup rollup -c && BABEL_ENV=rollup rollup -c --min",
    "dev": "webpack-dev-server --hot",
    "prepublish": "npm run build:es && npm run build:esnext && npm run build:node && npm run build:umd",
    "precommit": "lint-staged",
    "release": "git push --follow-tags && npm publish",
    "site": "webpack -p",
    "test:watch": "jest --watch",
    "test:ts": "tsc -p ./",
    "types": "get-typed src/index.js --dest umd/skatejs"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "setupFiles": [
      "./test/unit/setup"
    ],
    "setupTestFrameworkScriptFile": "./test/unit/setup-test",
    "testEnvironment": "@skatejs/ssr/jest"
  },
  "browser": "umd/index.js",
  "name": "skatejs",
  "version": "5.0.0-beta.3",
  "description": "Skate is a library built on top of the W3C web component specs that enables you to write functional and performant web components with a very small footprint.",
  "license": "MIT",
  "author": "Trey Shugart <treshugart@gmail.com> (http://treshugart.github.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/skatejs/skatejs"
  },
  "types": "src/index.d.ts",
  "keywords": [
    "components",
    "custom",
    "dom",
    "elements",
    "web",
    "vdom",
    "virtual"
  ],
  "lint-staged": {
    "*.(js|json)": [
      "prettier --write",
      "git add"
    ]
  }
}